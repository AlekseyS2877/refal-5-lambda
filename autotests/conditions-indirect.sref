// CONDITIONS

$ENTRY Go {
  = <Eq C <Test A B C D>>
    <Eq 0 <Test A B D>>;
}

Eq { s.X s.X = ; }

A { e.Any = #False; }

B {
  (e.Any) () = #True;
  e.Other = #False;
}

C {
  (A B) (D) = #True;
  e.Other = #False;
}

D {
  () (e.Any) = #True;
  e.Other = #False;
}

Test {
  e.Opened s.F e.Closed, <s.F (e.Opened) (e.Closed)> : #True = s.F;
  e.Other = 0;
}
